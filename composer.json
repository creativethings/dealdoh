{
  "name": "noglitchyo/dealdoh",
  "description": "A simple DNS over HTTPS proxy built on PHP.",
  "keywords": [
    "dns",
    "dns-resolver",
    "dns-over-https",
    "doh",
    "dns-proxy"
  ],
  "license": "MIT",
  "authors": [
    {
      "email": "maxime.elomari@gmail.com",
      "name": "Maxime Elomari"
    }
  ],
  "require": {
    "clue/socket-raw": "^1.4",
    "ext-json": "*",
    "ext-sockets": "*",
    "nyholm/psr7": "^1.1",
    "nyholm/psr7-server": "^0.3.0",
    "php": "^7.3",
    "psr/http-client": "*",
    "psr/log": "^1.1",
    "php-http/guzzle6-adapter": "^2.0",
    "react/dns": "^0.4.17"
  },
  "require-dev": {
    "slim/slim": "4.0.0-alpha",
    "slim/http": "^0.7.0",
    "phpunit/phpunit": "^8.1",
    "mockery/mockery": "^1.2",
    "monolog/monolog": "^1.24"
  },
  "autoload": {
    "psr-0": {
      "NoGlitchYo\\Dealdoh\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "NoGlitchYo\\Dealdoh\\": "tests/"
    }
  }
}
